    "lint": "eslint ."
  },
  "dependencies": {
    "@types/jest": "^25.1.2",
    "@types/react": "^16.9.19",
    "@types/react-native": "^0.61.12",
    "@types/react-test-renderer": "^16.9.2",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-modal": "git+ssh://git@github.com/reime005/react-native-modal#38351ea",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^0.0.7",
    "@types/detox": "^14.5.2",
    "babel-jest": "^25.1.0",
    "detox": "^15.2.1",
    "eslint": "^6.8.0",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.58.0",
    "react-test-renderer": "16.9.0"
  },
  "jest": {
    "preset": "react-native"
