{
  "name": "ReactNativeModalTest",
  "version": "0.0.1",
  "description": "Simple Test App for react-native-modal",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "@types/jest": "^26.0.9",
    "@types/react": "^16.9.19",
    "@types/react-native": "^0.61.12",
    "@types/react-test-renderer": "^16.9.2",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-modal": "git://github.com/reime005/react-native-modal#452cb50",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^0.0.7",
    "@types/detox": "^14.5.2",
    "babel-jest": "^25.1.0",
    "detox": "^17.3.3",
    "eslint": "^6.8.0",
    "jest": "^26.x.x",
    "jest-circus": "^26.x.x",
    "metro-react-native-babel-preset": "^0.58.0",
    "react-test-renderer": "16.9.0"
  },
  "jest": {
    "preset": "react-native"
  },
  "detox": {
    "specs": "e2e",
    "runnerConfig": "e2e/config.json",
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/ReactNativeModalTest.app",
        "build": "xcodebuild -workspace ios/ReactNativeModalTest.xcworkspace -scheme ReactNativeModalTest -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "device": {
          "type": "iPhone 11 Pro"
        }
      },
      "ios.sim.release": {
        "binaryPath": "ios/build/Build/Products/Release-iphonesimulator/ReactNativeModalTest.app",
        "build": "xcodebuild -workspace ios/ReactNativeModalTest.xcworkspace -scheme ReactNativeModalTest -configuration Release -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "device": {
          "type": "iPhone 11 Pro"
        }
      },
      "android.emu.release": {
        "binaryPath": "android/app/build/outputs/apk/release/app-release.apk",
        "build": "cd android; ./gradlew assembleRelease assembleAndroidTest -DtestBuildType=release",
        "type": "android.emulator",
          "device": {
            "avdName": "emu"
          }
      }
    },
    "test-runner": "jest"
  }
}
